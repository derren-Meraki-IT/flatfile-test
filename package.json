{
  "name": "flatfile-test",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@flatfile/api": "^1.7.2",
    "@flatfile/listener": "^1.0.1",
    "@flatfile/react": "^7.8.2",
    "@flatfile/v2-shims": "^1.1.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-scripts": "^2.1.3"
  },
  "scripts": {
    "del-problem-mjs": "del-cli node_modules/@flatfile/react/dist/index.browser.mjs && del-cli node_modules/@flatfile/v2-shims/dist/index.browser.mjs",
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts start",
    "start2": "set NODE_OPTIONS=--openssl-legacy-provider && npm run del-problem-mjs && react-scripts start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.2",
    "cross-env": "^7.0.3",
    "del-cli": "5.1.0",
    "typescript": "5.0.4"
  }
}
